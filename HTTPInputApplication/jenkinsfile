import groovy.json.JsonSlurperClassic

def jsonParse(def json) {
    new groovy.json.JsonSlurperClassic().parseText(json)
}
pipeline {
  agent { label 'master' }
  environment {
    appName = "variable" 
  }
  stages {

 stage("paso 1"){
     
      steps {
          script {			
           sh "echo 'Compilando Con Jenkins'"
           "sudo /home/vihupega/ace-11.0.0.11/tools/ ./mqsicreatebar" -data testIIB -b testIIB/HTTPInputApplication1.bar -a HTTPInputApplication -trace
           '''
        }
      }
    }
  }
  post {
      always {          
          deleteDir()
           sh "echo 'fase always'"
      }
      success {
            sh "echo 'Compilacion Exitosa'"
        }

      failure {
            sh "echo 'Compilacion Erronea'"
      }
      
  }
}
