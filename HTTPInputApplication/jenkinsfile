import groovy.json.JsonSlurperClassic

def jsonParse(def json) {
    new groovy.json.JsonSlurperClassic().parseText(json)
}
pipeline {
  agent any
  
  stages {
	stage("checkout code") {
         	steps {
            		deleteDir()
            		git 'https://github.com/vihupega/testIIB.git'
         	}
      	}
 	stage("build bar"){
     		steps {
        	  	script {
        	   		"/home/vihupega/ace-11.0.0.11/tools/ ./mqsicreatebar" -data . -b testIIB/HTTPInputApplication1.bar -a HTTPInputApplication -cleanBuild
        	   		'''
        		}
      		}
    	}
  }
  post {
      always {          
          deleteDir()
           sh "echo 'fase always'"
      }
      success {
            sh "echo 'Compilacion Exitosa'"
        }

      failure {
            sh "echo 'Compilacion Erronea'"
      }
      
  }
}
