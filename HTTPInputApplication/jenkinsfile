pipeline {
   agent any

   stages {
      stage('checkout code') {
         steps {
            deleteDir()
            git 'https://github.com/vihupega/testIIB.git'
         }
      }
      stage('build bar') {
        steps {
        bat label: '', script: '''
echo off
set projectName=testIIB
"/home/vihupega/ace-11.0.0.11/tools/ ./mqsicreatebar" -data . -b %projectName%.bar -a %projectName% -cleanBuild
'''
}
      }
      stage('deploy bar') {
 steps {
 bat label: '', script: '''
echo off
'''

archiveArtifacts 'testIIB.bar'
}
 }
   }
}
